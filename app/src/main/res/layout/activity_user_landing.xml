<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UserLanding"
    android:background="#DFEFEB">


    <Button
        android:id="@+id/program"
        android:layout_width="350dp"
        android:layout_height="77dp"
        android:layout_marginBottom="7dp"
        android:background="@drawable/input_bg"
        android:fontFamily="@font/poppins_light"
        android:text="Programming"
        android:textColor="#000000"
        android:textSize="16dp"
        app:backgroundTint="#FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/oss"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.508"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/Network"
        android:layout_width="350dp"
        android:layout_height="77dp"
        android:layout_marginBottom="7dp"
        android:background="@drawable/input_bg"
        android:fontFamily="@font/poppins_light"
        android:text="NETWORKING"
        android:textColor="#000000"
        android:textSize="16dp"
        app:backgroundTint="#FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/program"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.508"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/oss"
        android:layout_width="350dp"
        android:layout_height="77dp"
        android:layout_marginBottom="7dp"
        android:background="@drawable/input_bg"
        android:fontFamily="@font/poppins_light"
        android:text="Operating Systems"
        android:textColor="#000000"
        android:textSize="16dp"
        app:backgroundTint="#FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/construction1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.508"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="432dp"
        android:layout_height="303dp"
        android:layout_marginBottom="23dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/Network"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.523"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.685"
        app:srcCompat="@drawable/butn_get_started" />

    <TextView
        android:id="@+id/welcome2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins_light"
        android:text="Welcome"
        android:textColor="#FFFFFF"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/imageView9"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.852" />

    <TextView
        android:id="@+id/newappuser"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins_light"
        android:text="New Application User"
        android:textColor="#FFFFFF"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/imageView9"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.983" />

    <Button
        android:id="@+id/construction1"
        android:layout_width="350dp"
        android:layout_height="77dp"
        android:layout_marginBottom="7dp"
        android:background="@drawable/input_bg"
        android:fontFamily="@font/poppins_light"
        android:text="Under Construction"
        android:textColor="#000000"
        android:textSize="16dp"
        app:backgroundTint="#FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/construction2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.508"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/corner"
        />


    <Button

        android:id="@+id/construction2"
        android:layout_width="350dp"
        android:layout_height="77dp"
        android:layout_marginBottom="28dp"
        android:background="@drawable/input_bg"
        android:fontFamily="@font/poppins_light"
        android:text="UNDER CONSTRUCTION"
        android:textColor="#000000"
        android:textSize="16dp"
        app:backgroundTint="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.508"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="206dp"
        android:layout_height="163dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView9"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.424"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView9"
        app:layout_constraintVertical_bias="0.507"
        app:srcCompat="@drawable/temprof" />


</androidx.constraintlayout.widget.ConstraintLayout>